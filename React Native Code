#Homelogs
import React, { useCallback, useEffect, useState } from "react";
import { View, Text, StyleSheet, Pressable } from "react-native";
import type { NativeStackScreenProps } from "@react-navigation/native-stack";
import type { RootStackParamList } from "../../App";
import { supabase } from "../lib/supabase";
import { todayISO } from "../lib/dates";
import type { DayMetrics, ScoreRow } from "../types/db";

type Props = NativeStackScreenProps<RootStackParamList, "Home">;

// V1: hardcode user id for prototype (replace with Supabase Auth later)
const USER_ID = "demo-user-uuid-replace-me";

const defaultMetrics = (date: string): DayMetrics => ({
  user_id: USER_ID,
  date,
  morning_light_minutes: 0,
  morning_light_outdoor: false,
  morning_light_direct: false,
  morning_light_overcast: false,
  evening_screen_minutes: 0,
  hrv_ms: null,
  sleep_minutes: null,
});

export default function Home({ navigation }: Props) {
  const [score, setScore] = useState<ScoreRow | null>(null);
  const [metrics, setMetrics] = useState<DayMetrics | null>(null);

  const load = useCallback(async () => {
    const date = todayISO();

    const { data: m } = await supabase
      .from("day_metrics")
      .select("*")
      .eq("user_id", USER_ID)
      .eq("date", date)
      .maybeSingle();

    setMetrics((m as DayMetrics) ?? defaultMetrics(date));

    const { data: s } = await supabase
      .from("scores")
      .select("*")
      .eq("user_id", USER_ID)
      .eq("date", date)
      .maybeSingle();

    setScore((s as ScoreRow) ?? null);
  }, []);

  useEffect(() => {
    const unsub = navigation.addListener("focus", load);
    return unsub;
  }, [navigation, load]);

  const scoreValue = score?.score ?? 72; // fallback to your mock
  const summary =
    score?.summary ??
    "Great morning light yesterday, but too much screen time after sunset.";

  return (
    <View style={styles.container}>
      <View style={styles.headerRow}>
        <View>
          <Text style={styles.h1}>Good Morning, Sagar</Text>
          <Text style={styles.h2}>Today‚Äôs Energy Alignment Score: {scoreValue}</Text>
        </View>

        <Pressable onPress={() => navigation.navigate("Settings")} style={styles.gearBtn}>
          <Text style={{ fontSize: 18 }}>‚öôÔ∏è</Text>
        </Pressable>
      </View>

      <View style={styles.card}>
        <Text style={styles.quote}>‚Äú{summary}‚Äù</Text>
      </View>

      <Text style={styles.section}>Your Tasks Today:</Text>

      <View style={styles.task}>
        <Text style={styles.taskText}>üåÖ Get 20‚Äì30 min of morning sunlight</Text>
      </View>

      <View style={styles.task}>
        <Text style={styles.taskText}>üåô Keep screen use &lt; 60 min after sunset</Text>
      </View>

      <View style={styles.btnRow}>
        <Pressable style={styles.primary} onPress={() => navigation.navigate("LogMorningLight")}>
          <Text style={styles.primaryText}>Log Morning Light</Text>
        </Pressable>

        <Pressable style={styles.primary} onPress={() => navigation.navigate("LogEveningScreens")}>
          <Text style={styles.primaryText}>Log Evening Screens</Text>
        </Pressable>
      </View>

      <Pressable style={styles.secondary} onPress={() => navigation.navigate("Trends")}>
        <Text style={styles.secondaryText}>View Trends</Text>
      </Pressable>
    </View>
  );
}

const styles = StyleSheet.create({
  container: { flex: 1, padding: 20, gap: 14 },
  headerRow: { flexDirection: "row", justifyContent: "space-between", alignItems: "flex-start" },
  h1: { fontSize: 26, fontWeight: "800" },
  h2: { marginTop: 6, fontSize: 16, opacity: 0.85 },
  gearBtn: { padding: 10, borderRadius: 10, backgroundColor: "rgba(0,0,0,0.06)" },
  card: { padding: 14, borderRadius: 16, backgroundColor: "rgba(0,0,0,0.06)" },
  quote: { fontSize: 15, opacity: 0.9 },
  section: { marginTop: 8, fontSize: 16, fontWeight: "700" },
  task: { padding: 14, borderRadius: 16, backgroundColor: "rgba(0,0,0,0.04)" },
  taskText: { fontSize: 15 },
  btnRow: { flexDirection: "row", gap: 10, marginTop: 6 },
  primary: { flex: 1, padding: 14, borderRadius: 14, alignItems: "center", backgroundColor: "black" },
  primaryText: { color: "white", fontWeight: "700" },
  secondary: { marginTop: 6, padding: 14, borderRadius: 14, alignItems: "center", backgroundColor: "rgba(0,0,0,0.08)" },
  secondaryText: { fontWeight: "700" },
});

import React, { useMemo, useState } from "react";
import { View, Text, StyleSheet, Pressable, Switch } from "react-native";
import Slider from "@react-native-community/slider";
import { supabase } from "../lib/supabase";
import { todayISO } from "../lib/dates";
import { computeScore } from "../lib/score";
import type { DayMetrics } from "../types/db";
import type { NativeStackScreenProps } from "@react-navigation/native-stack";
import type { RootStackParamList } from "../../App";

type Props = NativeStackScreenProps<RootStackParamList, "LogMorningLight">;

const USER_ID = "demo-user-uuid-replace-me";

export default function LogMorningLight({ navigation }: Props) {
  const [minutes, setMinutes] = useState(20);
  const [outdoors, setOutdoors] = useState(true);
  const [direct, setDirect] = useState(false);
  const [overcast, setOvercast] = useState(false);
  const date = useMemo(() => todayISO(), []);

  async function onSave() {
    // Upsert day_metrics
    const { data: metricsRow, error } = await supabase
      .from("day_metrics")
      .upsert(
        {
          user_id: USER_ID,
          date,
          morning_light_minutes: Math.round(minutes),
          morning_light_outdoor: outdoors,
          morning_light_direct: direct,
          morning_light_overcast: overcast,
        },
        { onConflict: "user_id,date" }
      )
      .select("*")
      .single();

    if (error) {
      console.error(error);
      return;
    }

    const metrics = metricsRow as DayMetrics;
    const { score, summary } = computeScore({
      ...metrics,
      // ensure defaults if null/undefined returned
      evening_screen_minutes: metrics.evening_screen_minutes ?? 0,
      hrv_ms: metrics.hrv_ms ?? null,
      sleep_minutes: metrics.sleep_minutes ?? null,
    } as DayMetrics);

    // Upsert scores
    await supabase.from("scores").upsert(
      { user_id: USER_ID, date, score, summary },
      { onConflict: "user_id,date" }
    );

    navigation.goBack();
  }

  return (
    <View style={styles.container}>
      <Text style={styles.title}>Morning Light Exposure</Text>
      <Text style={styles.p}>How many minutes of outdoor light did you get this morning?</Text>

      <Text style={styles.value}>{Math.round(minutes)} min</Text>
      <Slider minimumValue={0} maximumValue={60} step={1} value={minutes} onValueChange={setMinutes} />

      <View style={styles.row}>
        <Text style={styles.label}>I was outdoors</Text>
        <Switch value={outdoors} onValueChange={setOutdoors} />
      </View>

      <View style={styles.row}>
        <Text style={styles.label}>Direct sunlight</Text>
        <Switch value={direct} onValueChange={(v) => { setDirect(v); if (v) setOvercast(false); }} />
      </View>

      <View style={styles.row}>
        <Text style={styles.label}>Overcast</Text>
        <Switch value={overcast} onValueChange={(v) => { setOvercast(v); if (v) setDirect(false); }} />
      </View>

      <Pressable style={styles.primary} onPress={onSave}>
        <Text style={styles.primaryText}>Save ‚Üí</Text>
      </Pressable>
    </View>
  );
}

const styles = StyleSheet.create({
  container: { flex: 1, padding: 20, gap: 14 },
  title: { fontSize: 22, fontWeight: "800" },
  p: { fontSize: 15, opacity: 0.8 },
  value: { marginTop: 10, fontSize: 20, fontWeight: "700" },
  row: { flexDirection: "row", justifyContent: "space-between", alignItems: "center", paddingVertical: 6 },
  label: { fontSize: 16 },
  primary: { marginTop: 18, padding: 14, borderRadius: 14, alignItems: "center", backgroundColor: "black" },
  primaryText: { color: "white", fontSize: 16, fontWeight: "700" },
});

import React, { useMemo, useState } from "react";
import { View, Text, StyleSheet, Pressable } from "react-native";
import Slider from "@react-native-community/slider";
import { supabase } from "../lib/supabase";
import { todayISO } from "../lib/dates";
import { computeScore } from "../lib/score";
import type { DayMetrics } from "../types/db";
import type { NativeStackScreenProps } from "@react-navigation/native-stack";
import type { RootStackParamList } from "../../App";

type Props = NativeStackScreenProps<RootStackParamList, "LogEveningScreens">;

const USER_ID = "demo-user-uuid-replace-me";

export default function LogEveningScreens({ navigation }: Props) {
  const [minutes, setMinutes] = useState(60);
  const date = useMemo(() => todayISO(), []);

  async function onSave() {
    const { data: metricsRow, error } = await supabase
      .from("day_metrics")
      .upsert(
        { user_id: USER_ID, date, evening_screen_minutes: Math.round(minutes) },
        { onConflict: "user_id,date" }
      )
      .select("*")
      .single();

    if (error) {
      console.error(error);
      return;
    }

    const metrics = metricsRow as DayMetrics;
    const { score, summary } = computeScore({
      ...metrics,
      morning_light_minutes: metrics.morning_light_minutes ?? 0,
      morning_light_outdoor: metrics.morning_light_outdoor ?? false,
      morning_light_direct: metrics.morning_light_direct ?? false,
      morning_light_overcast: metrics.morning_light_overcast ?? false,
      hrv_ms: metrics.hrv_ms ?? null,
      sleep_minutes: metrics.sleep_minutes ?? null,
    });

    await supabase.from("scores").upsert(
      { user_id: USER_ID, date, score, summary },
      { onConflict: "user_id,date" }
    );

    navigation.goBack();
  }

  return (
    <View style={styles.container}>
      <Text style={styles.title}>Screen Exposure After Sunset</Text>
      <Text style={styles.p}>How much time did you spend on screens after sunset?</Text>

      <Text style={styles.value}>{Math.round(minutes)} min</Text>
      <Slider minimumValue={0} maximumValue={180} step={1} value={minutes} onValueChange={setMinutes} />

      <Pressable style={styles.primary} onPress={onSave}>
        <Text style={styles.primaryText}>Save ‚Üí</Text>
      </Pressable>
    </View>
  );
}

const styles = StyleSheet.create({
  container: { flex: 1, padding: 20, gap: 14 },
  title: { fontSize: 22, fontWeight: "800" },
  p: { fontSize: 15, opacity: 0.8 },
  value: { marginTop: 10, fontSize: 20, fontWeight: "700" },
  primary: { marginTop: 18, padding: 14, borderRadius: 14, alignItems: "center", backgroundColor: "black" },
  primaryText: { color: "white", fontSize: 16, fontWeight: "700" },
});

#Trends/Weekly summary
import React, { useState } from "react";
import { View, Text, StyleSheet, Pressable } from "react-native";
import type { NativeStackScreenProps } from "@react-navigation/native-stack";
import type { RootStackParamList } from "../../App";

type Props = NativeStackScreenProps<RootStackParamList, "Trends">;

export default function Trends({ navigation }: Props) {
  const [tab, setTab] = useState<"sleep" | "hrv" | "weekly">("hrv");

  return (
    <View style={styles.container}>
      <View style={styles.tabs}>
        <Pressable style={[styles.tab, tab === "sleep" && styles.tabActive]} onPress={() => setTab("sleep")}>
          <Text style={[styles.tabText, tab === "sleep" && styles.tabTextActive]}>Light vs Sleep</Text>
        </Pressable>
        <Pressable style={[styles.tab, tab === "hrv" && styles.tabActive]} onPress={() => setTab("hrv")}>
          <Text style={[styles.tabText, tab === "hrv" && styles.tabTextActive]}>Light vs HRV</Text>
        </Pressable>
        <Pressable style={[styles.tab, tab === "weekly" && styles.tabActive]} onPress={() => navigation.navigate("WeeklySummary")}>
          <Text style={styles.tabText}>Weekly Summary</Text>
        </Pressable>
      </View>

      <Text style={styles.title}>How Light Affects Your HRV</Text>

      <View style={styles.chartPlaceholder}>
        <Text style={{ opacity: 0.7 }}>[Graph placeholder: HRV line + morning light bars]</Text>
      </View>

      <View style={styles.insight}>
        <Text style={styles.insightText}>
          ‚ÄúOn days you get ‚â•20 min of morning light, your HRV is 10‚Äì18% higher.‚Äù
        </Text>
      </View>
    </View>
  );
}

const styles = StyleSheet.create({
  container: { flex: 1, padding: 20, gap: 14 },
