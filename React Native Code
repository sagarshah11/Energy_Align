  chartPlaceholder: { height: 220, borderRadius: 16, backgroundColor: "rgba(0,0,0,0.06)", justifyContent: "center", alignItems: "center" },
  insight: { padding: 14, borderRadius: 16, backgroundColor: "rgba(0,0,0,0.06)" },
  insightText: { fontSize: 15, opacity: 0.9 },
});

