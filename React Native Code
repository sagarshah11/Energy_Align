#Supabase client
import { createClient } from "@supabase/supabase-js";

// Put these in app config later (env). Hardcoding is fine for V1 prototype.
const SUPABASE_URL = "https://YOUR_PROJECT.supabase.co";
const SUPABASE_ANON_KEY = "YOUR_ANON_KEY";

export const supabase = createClient(SUPABASE_URL, SUPABASE_ANON_KEY);
export type DayMetrics = {
  user_id: string;
  date: string; // YYYY-MM-DD
  morning_light_minutes: number;
  morning_light_outdoor: boolean;
  morning_light_direct: boolean;
  morning_light_overcast: boolean;
  evening_screen_minutes: number;
  hrv_ms: number | null;
  sleep_minutes: number | null;
};

export type ScoreRow = {
  user_id: string;
  date: string;
  score: number;
  summary: string;
};
export function todayISO(): string {
  return new Date().toISOString().slice(0, 10);
}
import type { DayMetrics } from "../types/db";

export function computeScore(metrics: DayMetrics): { score: number; summary: string } {
  let score = 50;
  const notes: string[] = [];

  if (metrics.morning_light_minutes >= 20) {
    score += 20;
    notes.push("Great morning light yesterday");
  } else {
    notes.push("Try to get 20‚Äì30 min of morning light");
  }

  if (metrics.evening_screen_minutes <= 60) {
    score += 20;
    notes.push("Nice job limiting screens after sunset");
  } else {
    notes.push("Too much screen time after sunset");
  }

  // Optional: add small boosts if wearable data exists
  if (metrics.sleep_minutes != null && metrics.sleep_minutes >= 7 * 60) score += 5;
  if (metrics.hrv_ms != null && metrics.hrv_ms >= 45) score += 5;

  const clamped = Math.max(0, Math.min(100, score));
  return { score: clamped, summary: notes.join(", ") + "." };
}
#Navigation
import React from "react";
import { NavigationContainer } from "@react-navigation/native";
import { createNativeStackNavigator } from "@react-navigation/native-stack";

import Splash from "./src/screens/Splash";
import Onboarding1 from "./src/screens/Onboarding1";
import Onboarding2 from "./src/screens/Onboarding2";
import Onboarding3 from "./src/screens/Onboarding3";
import Home from "./src/screens/Home";
import LogMorningLight from "./src/screens/LogMorningLight";
import LogEveningScreens from "./src/screens/LogEveningScreens";
import Trends from "./src/screens/Trends";
import WeeklySummary from "./src/screens/WeeklySummary";
import Settings from "./src/screens/Settings";

export type RootStackParamList = {
  Splash: undefined;
  Onboarding1: undefined;
  Onboarding2: undefined;
  Onboarding3: undefined;
  Home: undefined;
  LogMorningLight: undefined;
  LogEveningScreens: undefined;
  Trends: undefined;
  WeeklySummary: undefined;
  Settings: undefined;
};

const Stack = createNativeStackNavigator<RootStackParamList>();

export default function App() {
  return (
    <NavigationContainer>
      <Stack.Navigator initialRouteName="Splash">
        <Stack.Screen name="Splash" component={Splash} options={{ headerShown: false }} />
        <Stack.Screen name="Onboarding1" component={Onboarding1} options={{ title: "" }} />
        <Stack.Screen name="Onboarding2" component={Onboarding2} options={{ title: "" }} />
        <Stack.Screen name="Onboarding3" component={Onboarding3} options={{ title: "" }} />
        <Stack.Screen name="Home" component={Home} options={{ title: "EnergyAlign" }} />
        <Stack.Screen name="LogMorningLight" component={LogMorningLight} options={{ title: "Morning Light" }} />
        <Stack.Screen name="LogEveningScreens" component={LogEveningScreens} options={{ title: "Evening Screens" }} />
        <Stack.Screen name="Trends" component={Trends} options={{ title: "Trends" }} />
        <Stack.Screen name="WeeklySummary" component={WeeklySummary} options={{ title: "Weekly Summary" }} />
        <Stack.Screen name="Settings" component={Settings} options={{ title: "Settings" }} />
      </Stack.Navigator>
    </NavigationContainer>
  );
}
#Screens
import React, { useEffect } from "react";
import { View, Text, StyleSheet } from "react-native";
import type { NativeStackScreenProps } from "@react-navigation/native-stack";
import type { RootStackParamList } from "../../App";

type Props = NativeStackScreenProps<RootStackParamList, "Splash">;

export default function Splash({ navigation }: Props) {
  useEffect(() => {
    const t = setTimeout(() => navigation.replace("Onboarding1"), 900);
    return () => clearTimeout(t);
  }, [navigation]);

  return (
    <View style={styles.container}>
      <Text style={styles.title}>EnergyAlign</Text>
      <Text style={styles.subtitle}>Align your environment with your biology.</Text>
    </View>
  );
}

const styles = StyleSheet.create({
  container: { flex: 1, justifyContent: "center", alignItems: "center", padding: 24 },
  title: { fontSize: 40, fontWeight: "800" },
  subtitle: { marginTop: 10, fontSize: 16, opacity: 0.75, textAlign: "center" },
});

